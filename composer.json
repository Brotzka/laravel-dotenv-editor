{
    "name": "chinmoym2004/laravel-dotenv-editor",
    "description": "A package for editing the .env file in your Laravel root. This copy has the support for Laravel 6,7 & bootstrap 4 UI",
    "keywords": ["laravel", "dotenv", "dotenv-editor"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Hagen",
            "email": "fabianhagen87@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "Brotzka\\DotenvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brotzka\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "Brotzka\\DotenvEditor\\DotenvEditorFacade"
            }
        }
    }
}
